<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema path="/org/gnome/shell/extensions/miniview/" id="org.gnome.shell.extensions.miniview" gettext-domain="gnome-shell">
    <key name="toggle-miniview" type="as">
      <default><![CDATA[['<Shift>F12']]]></default>
      <summary>Toggle Miniview</summary>
      <description>Toggle the Miniview Gnome shell extension.</description>
    </key>
    <key name="showind" type="b">
      <default>true</default>
      <summary>Show panel menu</summary>
      <description>Show the Miniview panel menu.</description>
    </key>
    <key name="showme" type="b">
      <default>true</default>
      <summary>Show Miniview</summary>
      <description>Show Miniview window.</description>
    </key>
    <key name="window-name-filter" type="s">
      <default>""</default>
      <summary>Filter selected windows based on this filter</summary>
      <description>Filter selected windows based on this filter</description>
    </key>
  </schema>
</schemalist>
